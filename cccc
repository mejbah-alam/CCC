#include <iostream>
using namespace std;
int main()
{
    int n, row, col;
    cout << "Enter the Number of Rows: ";
    cin >> n;
    for(row = 1; row <= n; row++)
    {
        for(col = 1; col <= row; col++)
        {
            cout << row % 2 << " ";
        }
        cout << endl;
    }
    return 0;
}
\end{lstlisting}

