    if((ch >= 97 && ch <= 122) || (ch >= 65 && ch <= 90))
    {
        cout << "'" << ch << "' is Alphabet." << endl;
    }
    else if(ch >= 48 && ch <= 57)
    {
        cout << "'" << ch << "' is Digit." << endl;
    }
    else
    {
        cout << "'" << ch << "' is Special Character." << endl;
    }
    return 0;
}
