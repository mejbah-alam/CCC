\subsection{Print n Number Using Do While Loop}
\vspace{0.5cm}
\begin{lstlisting}[caption={Print n Number Using Do While Loop}]
#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
    int i = 1, n;
    cout << "Enter the Number of Series: ";
    cin >> n;
    do
    {
        cout << setw(2) << i << "\n";
        i++;
    } while (i <= n);
    return 0;
}
\end{lstlisting}

