#include <iostream>
using namespace std;
int main()
{
    int num, n;
    cout << "Enter a Number: ";
    cin >> num;
    cout << "Enter the Bit Position to Toggle (0-based): ";
    cin >> n;
    num = num ^ (1 << n);
    cout << "After Toggling the " << n << "th Bit, the Number is: " << num << endl;
    return 0;
}
\end{lstlisting}
