\vspace{0.5cm}
\begin{lstlisting}[caption={Area of Square}]
#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
    float side, area;
    cout << "Enter Side: ";
    cin >> side;
    area = side * side;
    cout << "The Area is: " << fixed << setprecision(6) << area;
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
Enter Side: 6
The Area is: 36.000000
\end{verbatim}
