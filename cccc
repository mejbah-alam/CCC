{
    string A = "Mejbahul Alam";
    string B = "Mejbahul";
    if(A == B)
    {
        cout << "The Strings are Equal";
    }
    else
    {
        cout << "The Strings are not Equal";
    }
    return 0;
}
\end{lstlisting}

