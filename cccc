#include <string>
using namespace std;
int main()
{
    string A = "Mejbahul Alam";
    string B;
    B = A;
    cout << "A is: " << A << endl;
    cout << "B is: " << B << endl;
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
A is: Mejbahul Alam
B is: Mejbahul Alam
