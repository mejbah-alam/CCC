\subsection{Convert Octal to Hexadecimal Number System}
\vspace{0.5cm}
\begin{lstlisting}[caption={Convert Octal to Hexadecimal Number System}]
#include <iostream>
#include <iomanip>
#include <vector>
#include <cmath>
using namespace std;
int main()
{
    int oct, dec = 0, i = 0;
    vector<int> hex;
    char hexChar[] = "0123456789ABCDEF";
    cout << "Enter an octal number: ";
    cin >> oct;
    while (oct != 0)
    {
        dec += (oct % 10) * pow(8, i);
        i++;
        oct /= 10;
    }
    while (dec > 0)
    {
        hex.push_back(dec % 16);
        dec /= 16;
    }
    cout << "Hexadecimal number: ";
    for (int k = hex.size() - 1; k >= 0; k--)
        cout << hexChar[hex[k]];
    cout << "\n";
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
Enter an octal number: 15
Hexadecimal number: D
\end{verbatim}
