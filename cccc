\subsection{Print Number Diamond Shape Pattern}
\vspace{0.5cm}
\begin{lstlisting}[caption={Print Number Diamond Shape Pattern}]
#include <iostream>
using namespace std;
int main()
{
    int n, row, col;
    cout << "Enter the Number of Rows: ";
    cin >> n;
    for(row = 1; row <= n; row++)
    {
        for(col = 1; col <= n - row; col++)
        {
            cout << "  ";
        }
        for(col = 1; col <= row; col++)
        {
            cout << " " << col;
        }
        for(col = row - 1; col >= 1; col--)
        {
            cout << " " << col;
        }
        cout << endl;
    }
    for(row = n - 1; row >= 1; row--)
    {
        for(col = 1; col <= n - row; col++)
        {
            cout << "  ";
        }
        for(col = 1; col <= row; col++)
        {
            cout << " " << col;
        }
        for(col = row - 1; col >= 1; col--)
        {
            cout << " " << col;
        }
        cout << endl;
    }
    return 0;
}
\end{lstlisting}

\textbf{Output for n=5:}
\begin{verbatim}
        1
      1 2 1
    1 2 3 2 1
  1 2 3 4 3 2 1
1 2 3 4 5 4 3 2 1
  1 2 3 4 3 2 1
    1 2 3 2 1
      1 2 1
        1
\end{verbatim}
