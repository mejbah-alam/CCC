    for(i = 0; i < n; i++)
    {
        cout << "Enter " << i + 1 << " Number: ";
        cin >> num[i];
    }
    min = num[0];
    for(i = 0; i < n; i++)
    {
        if(min > num[i])
            min = num[i];
