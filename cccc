#include <iostream>
using namespace std;
int main()
{
    char ch;
    cout << "Enter Any Character: ";
    cin >> ch;
    ((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z')) ? 
        cout << ch << " is a Alphabet" << endl : cout << ch << " is Not a Alphabet" << endl;
    return 0;
}
\end{lstlisting}
\textbf{Output:}
\begin{verbatim}
Enter Any Character: b
b is a Alphabet
\end{verbatim}
