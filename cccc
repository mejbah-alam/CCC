#include <iostream>
using namespace std;
int main()
{
    int num1, num2, num3;
    cout << "Enter three numbers: ";
    cin >> num1 >> num2 >> num3;
    if(num1 <= num2)
    {
        if(num1 <= num3)
            cout << num1 << " is the Smallest Number" << endl;
        else
            cout << num3 << " is the Smallest Number" << endl;
    }
    else
    {
        if(num2 <= num3)
            cout << num2 << " is the Smallest Number" << endl;
        else
            cout << num1 << " is the Smallest Number" << endl;
