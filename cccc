\vspace{0.5cm}
\begin{lstlisting}[caption={Positive or Negative Using If-else if-else}]
#include <iostream>
using namespace std;
int main()
{
    int num;
    cout << "Enter an Integer: ";
    cin >> num;
    if(num > 0)
        cout << "The Number is Positive" << endl;
    else if(num < 0)
        cout << "The Number is Negative" << endl;
    else
        cout << "The Number is Zero" << endl;
    return 0;
}
\end{lstlisting}
\textbf{Output:}
\begin{verbatim}
