\vspace{0.5cm}
\begin{lstlisting}[caption={Print n Number Using While Loop}]
#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
    int i = 1, n;
    cout << "Enter the Number of Series: ";
    cin >> n;
    while (i <= n)
    {
        cout << setw(2) << i << "\n";
        i++;
    }
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
