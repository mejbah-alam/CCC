#include <iostream>
using namespace std;
int main()
{
    int n;
    cout << "Size of Array: ";
    cin >> n;
    cout << endl;
    if (n > 100)
    {
        cout << "Array size too large!" << endl;
        return 1;
    }
    int arr[100];
    for (int i = 0; i < n; i++)
    {
        cout << "Enter " << i + 1 << " Element: ";
