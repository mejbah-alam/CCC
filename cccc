\subsection{String Concatenation}
\vspace{0.5cm}
\begin{lstlisting}[caption={Concatenate Strings}]
#include <iostream>
#include <string>
using namespace std;
int main()
{
    string A = "My name is ";
    string B = "Mejbahul Alam";
    A += B;
    cout << "String 1: " << A << endl;
    cout << "String 2: " << B << endl;
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
String 1: My name is Mejbahul Alam
String 2: Mejbahul Alam
\end{verbatim}
