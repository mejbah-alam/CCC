#include <vector>
using namespace std;
int main()
{
    int dec;
    vector<int> bin;
    cout << "Enter a decimal number: ";
    cin >> dec;
    while (dec > 0)
    {
        bin.push_back(dec % 2);
        dec /= 2;
    }
    cout << "Binary number: ";
    for (int j = bin.size() - 1; j >= 0; j--)
        cout << bin[j];
    cout << "\n";
    return 0;
}
