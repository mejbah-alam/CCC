\subsection{Print Even Number Using For Loop}
\vspace{0.5cm}
\begin{lstlisting}[caption={Print Even Number Using For Loop}]
#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
    int i, n;
    cout << "Enter the Last Number: ";
    cin >> n;
    for (i = 2; i <= n; i += 2)
    {
        cout << setw(2) << i << "\n";
    }
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
Enter the Last Number: 6
2
4
6
\end{verbatim}
