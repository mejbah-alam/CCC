\vspace{0.5cm}
\begin{lstlisting}[caption={Print Inverted Number Pyramid Shape Pattern}]
#include <iostream>
using namespace std;
int main()
{
    int n, row, col;
    cout << "Enter the Number of Rows: ";
    cin >> n;
    for(row = n; row >= 1; row--)
    {
        for(col = 1; col <= n - row; col++)
        {
            cout << "  ";
        }
        for(col = 1; col <= row; col++)
        {
            cout << " " << col;
        }
        for(col = row - 1; col >= 1; col--)
