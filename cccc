\end{verbatim}
\newpage

\subsubsection{Print Alphabet \{A, B, C, ..., Z\} - Column Right Angle Triangle}
\vspace{0.5cm}
\begin{lstlisting}[caption={Print Alphabet \{A, B, C, ..., Z\} - Column Right Angle Triangle}]
#include <iostream>
using namespace std;
int main()
{
    int n, row, col;
    cout << "Enter the Number of Rows: ";
    cin >> n;
    for(row = 1; row <= n; row++)
    {
        for(col = 1; col <= row; col++)
        {
            cout << char(col + 64) << " ";
        }
        cout << endl;
    }
    return 0;
}
\end{lstlisting}

\textbf{Output for n=5:}
\begin{verbatim}
A 
A B 
A B C 
A B C D 
A B C D E 
\end{verbatim}
\newpage

