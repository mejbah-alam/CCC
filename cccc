\subsection{Largest of Two Numbers Using If-else if-else}
\vspace{0.5cm}
\begin{lstlisting}[caption={Largest of Two Numbers Using If-else if-else}]
#include <iostream>
using namespace std;
int main()
{
    int num1, num2;
    cout << "Enter 1st Number: ";
    cin >> num1;
    cout << "Enter 2nd Number: ";
    cin >> num2;
    if(num1 > num2)
        cout << "1st Number is Largest" << endl;
    else if(num2 > num1)
        cout << "2nd Number is Largest" << endl;
    else
        cout << "The Numbers are Equal" << endl;
    return 0;
}
\end{lstlisting}
\textbf{Output:}
\begin{verbatim}
Enter 1st Number: 10
Enter 2nd Number: 20
2nd Number is Largest
\end{verbatim}
