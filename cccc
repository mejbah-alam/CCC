#include <iostream>
#include <string>
using namespace std;
int main()
{
    string Name = "Mejbahul Alam";
    int len = Name.length();
    cout << "Length of the String is: " << len;
    return 0;
}
\end{lstlisting}

\textbf{Output:}
\begin{verbatim}
Length of the String is: 13
\end{verbatim}
